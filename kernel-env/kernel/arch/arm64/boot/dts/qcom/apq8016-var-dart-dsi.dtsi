/ {
	backlight: backlight {
		compatible = "gpio-backlight";
		gpios = <&pm8916_mpps 4 GPIO_ACTIVE_HIGH>;
		default-on;
	};
};

&dsi0_out {
	remote-endpoint = <&panel_in>;
	data-lanes = <0 1 2 3>;
};


&mdss_dsi0 {
    #address-cells = <1>;
    #size-cells = <0>;
 	status = "ok";
	panel:panel@0{
		reg = <0>;
		status = "ok";
		compatible = "fet,f99115a";
		backlight = <&backlight>;
		power-supply = <&vph_pwr_vreg>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&dsi0_out>;
			};
		};
	};

};